{
  "name": "@heroui/use-aria-modal-overlay",
  "version": "2.2.17",
  "description": "A custom implementation of react aria modal overlay, this removes the prevent scroll",
  "keywords": [
    "use-aria-modal-overlay"
  ],
  "author": "HeroUI <support@heroui.com>",
  "homepage": "https://heroui.com",
  "license": "MIT",
  "main": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heroui-inc/heroui.git",
    "directory": "packages/hooks/use-aria-modal-overlay"
  },
  "bugs": {
    "url": "https://github.com/heroui-inc/heroui/issues"
  },
  "dependencies": {
    "@react-aria/overlays": "3.28.0",
    "@react-aria/utils": "3.30.0",
    "@react-stately/overlays": "3.6.18",
    "@heroui/use-aria-overlay": "2.0.2"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-rc.0",
    "react-dom": ">=18 || >=19.0.0-rc.0"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit"
  }
}